    "babel-preset-latest": "^6.0.0",
    "jsdom": "^11.6.1",
    "jsdom-global": "^3.0.2",
    "marked": "^0.3.12",
    "mocha": "^5.0.0",
    "mocha-webpack": "^1.0.1",
    "should": "^13.2.1",
    "standard": "^10.0.3",
    "vue-cli": "^2.8.2",
    "vue-loader": "^12.0.0",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.0",
    "webpack-node-externals": "^1.6.0"
  },
  "standard": {
    "ignore": [
      "/lib/*",
      "/examples/*"
    ],
    "env": {
      "mocha": true
