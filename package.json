{
  "name": "propdoc",
  "description": "quick and easy documentation of Vue.js components",
  "version": "0.9.1",
  "author": "Dave Honneffer <pearofducks@gmail.com>",
  "private": false,
  "main": "lib/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/propellant/doctor.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "propellant",
    "propdoc",
    "documentation",
    "component"
  ],
  "scripts": {
    "release": "vue build ./index.js --prod --dist lib --lib propDoc",
